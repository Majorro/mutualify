{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/main.log",
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Month"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "PathBase": "/",
  "osuApi": {
    "ClientId": 0,
    "ClientSecret": "",
    "CallbackUrl": ""
  },
  "Database": {
    "Database": "mutualify",
    "Host": "localhost",
    "Port": 5432,
    "Username": "postgres",
    "Password": "postgres"
  }
}
